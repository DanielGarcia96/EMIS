#!/usr/bin/perl

use warnings;

$n_14400  = 0;
$ft_19200 = 0;
$nt_28800 = 0;
$tw_33600 = 0;
$a33600 = 0;
while(<>){
    if(s/^.*Ascend-Data-Rate = (.*)/$1/){
        $n_14400++ if $_ <= 14400;
        $ft_19200++ if $_ >= 14401 and $_ <= 19200;
        $nt_28800++ if $_ >= 19201 and $_ <= 28800;
        $tw_33600++ if $_ >= 28801 and $_ <= 33600;
        $a33600++ if $_ > 33600;
    }
}

print "0-14400\t\t$n_14400\n";
print "14401-19200\t$ft_19200\n";
print "19201-28800\t$nt_28800\n";
print "28801-33600\t$tw_33600\n";
print "above 33600\t$a33600\n";
